{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "kdashboard-ui": {
      "projectType": "library",
      "root": "",
      "sourceRoot": "src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@vcd/plugin-builders:plugin-builder",
          "options": {
            "modulePath": "src/main/skeleton.module.ts#SkeletonModule",
            "outputPath": "dist/",
            "index": "src/index.html",
            "main": "src/main/index.ts",
            "tsConfig": "tsconfig.json",
            "ignoreDefaultExternals": true,
            "externalLibs": [
              "@angular/common$",
              "@angular/common/http$",
              "@angular/core$",
              "@angular/forms$",
              "@angular/platform-browser$",
              "@angular/router$",
              "@clr/angular$",
              "@ngrx/store$",
              "^@vcd\/common$",
              "^@vcd-ui\/common$",
              "rxjs$",
              "rxjs/internal-compatibility$",
              "rxjs/operators$"
            ],
            "assets": [
              {
                "glob": "**/*",
                "input": "./src/public",
                "output": "/"
              }
            ],
            "optimization": true,
            "outputHashing": "all",
            "sourceMap": false,
            "extractCss": false,
            "namedChunks": false,
            "aot": false,
            "extractLicenses": false,
            "vendorChunk": false,
            "buildOptimizer": false
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.lib.json",
              "tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "kdashboard-ui",
  "cli": {
    "analytics": false
  }
}
